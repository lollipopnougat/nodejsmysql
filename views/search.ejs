            </div>
            <button class="layui-btn layui-bg-orange" lay-submit="" lay-filter="search"
              style="margin: 4px 4px 5px 5px;">立即搜索</button>
          </form>
        </div>
        <script>
          layui.use(['form', 'jquery'], function () {
            var form = layui.form;
            var $ = layui.jquery;
            form.on('submit(search)', function (data) {
              location.href = '/search?cname=' + data.field.search;
              return false;
            });
          });
        </script>
        <div class="clear"></div>
      </div>
